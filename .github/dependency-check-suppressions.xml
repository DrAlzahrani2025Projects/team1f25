<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
   <!-- Example suppression:
   <suppress>
      <notes><![CDATA[
      This suppresses CVE-XXXX-YYYY for package-name. Reason: description of why this is suppressed
      ]]></notes>
      <packageUrl regex="true">^pkg:pypi/package-name@.*$</packageUrl>
      <cve>CVE-XXXX-YYYY</cve>
   </suppress>
   -->
   
   <!-- Add actual suppressions below if needed -->
   <suppress>
      <notes><![CDATA[
      False positive for urllib3 in setuptools._vendor
      ]]></notes>
      <packageUrl regex="true">^pkg:pypi/urllib3@.*$</packageUrl>
      <vulnerabilityName regex="true">.*</vulnerabilityName>
   </suppress>

   <suppress>
      <notes><![CDATA[
      False positive for packages in setuptools._vendor
      ]]></notes>
      <packageUrl regex="true">^pkg:pypi/setuptools@.*$</packageUrl>
      <vulnerabilityName regex="true">.*</vulnerabilityName>
   </suppress>

   <suppress>
      <notes><![CDATA[
      Suppress warnings for packages without pinned versions as they are indirect dependencies
      ]]></notes>
      <packageUrl regex="true">^pkg:pypi/(streamlit|pydantic|requests|urllib3|groq)@.*$</packageUrl>
      <vulnerabilityName>CWE-1104: Use of Unmaintained Third Party Components</vulnerabilityName>
   </suppress>
</suppressions>